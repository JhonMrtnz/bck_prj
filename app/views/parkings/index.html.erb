<p id="notice"><%= notice %></p>

<h1>Lista de Parkeos</h1>
<div style="overflow-x:auto; width:850px">
<table>
  <thead>
    <tr>
       
      <th>Nombre</th>
      <th>Direccion</th>
      <th>Correo</th>
      <th>Telefono</th>
      <th>Movil</th>
      <th>Costoxdia</th>
      <th>Costoxhora</th>
      <th>Distrito</th>
      <th>Agregado</th>
      <th>Imagen</th>
      <th colspan="3"></th>
     
    </tr>
  </thead>

  <tbody>
    <% @parkings.each do |parking| %>
      <tr>
    
        <td><%= parking.nombre %></td>
        <td><%= parking.direccion %></td>
        <td><%= parking.correo %></td>
        <td><%= parking.telefono %></td>
        <td><%= parking.movil %></td>
        <td><%= parking.costoxdia %></td>
        <td><%= parking.costoxhora %></td>
        <td><%= parking.distrito.nombre %></td>
        <td><%= parking.agregado.descripcion %></td>
        <td><%= image_tag parking.avatar.url(:thumb) %></td>
             <td><%= link_to 'Show', parking %></td>
        <td><%= link_to 'Edit', edit_parking_path(parking) %></td>
        <td><%= link_to 'Destroy', parking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Parking', new_parking_path %>
